{
  "name": "xbftw-bot",
  "description": "XBFTW-A Multipurpose Discord Bot",
  "repository": "https://github.com/XBFTW/XBFTW-BOT",
  "logo": "https://xbftw.com/assets/img/xbftw.png",
  "keywords": ["discord", "discordjs", "music"],
  "env": {    
    "MONGO_URI": {
     "description": "Your Mongo url",
     "value": "",
     "required": true
    },
    "NODE_HOST":{
      "description":"lavalink host",
      "value": "disbotlistlavalink.ml",
      "required": true
    },
    "NODE_ID":{
      "description":"lavalink ID",
      "value": "main",
      "required": true
    },
    "NODE_PORT":{
      "description":"lavalink port",
      "value": "443",
      "required": true
    },
    "NODE_PASSWORD":{
      "description":"lavalink password",
      "value": "LAVA",
      "required": true
    },
    "NODE_SECURE":{
      "description":"lavalink secure (true/false)",
      "value": "true",
      "required": true
    },
    "PREFIX":{
      "description": "your prefix",
      "value": "!",
      "required": true
     },
    "COlOR":{
      "description": "embed colour code hex",
      "value": "#303236",
      "required": false
     },
    "LOGS":{
      "description": "Channel I'd",
      "required": true
     },
    "OWNERID":{
      "description": "your discord id",
      "required": false
     },
    "TOKEN": {
      "description": "Your bot's bot token.",
      "required": true
    }
  }
}